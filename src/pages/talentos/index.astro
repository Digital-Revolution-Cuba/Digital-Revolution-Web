---
import StructuredData from "../../components/seo/StructuredData.astro";
import TalentsSearch from "../../components/talents/TalentsSearch.astro";
import Layout from "../../layouts/Layout.astro";
import "../../styles/components/talents.css";

export const prerender = true;
---

<Layout
  title="Talentos Creativos de Cuba — Digital Revolution Web"
  description="Explora la comunidad de talentos creativos de Cuba: artistas, músicos, desarrolladores y más. Encuentra colaboradores y oportunidades en Digital Revolution."
  ogType="website"
  ogImageType="talentos"
>
  <StructuredData
    type="collection"
    title="Talentos Creativos de Cuba"
    description="Comunidad de talentos creativos de Cuba en Digital Revolution Web."
    breadcrumbs={[
      { name: "Inicio", url: "/" },
      { name: "Talentos", url: "/talentos" },
    ]}
  />
  <TalentsSearch />
</Layout>
